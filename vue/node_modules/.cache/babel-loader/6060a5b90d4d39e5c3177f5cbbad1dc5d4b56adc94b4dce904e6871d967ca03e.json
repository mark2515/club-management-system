{"ast":null,"code":"export default {\n  name: \"User\",\n  data() {\n    return {\n      tableData: [],\n      total: 0,\n      pageNum: 1,\n      pageSize: 10,\n      username: \"\",\n      email: \"\",\n      address: \"\",\n      form: {},\n      dialogFormVisible: false,\n      multipleSelection: []\n    };\n  },\n  created() {\n    this.load();\n  },\n  methods: {\n    load() {\n      this.request.get(\"http://localhost:9090/user/page?pageNum=\" + this.pageNum + \"&pageSize=\" + this.pageSize + \"&username=\" + this.username + \"&email=\" + this.email + \"&address=\" + this.address).then(res => {\n        console.log(res);\n        this.tableData = res.records;\n        this.total = res.total;\n      });\n    },\n    save() {\n      this.request.post(\"http://localhost:9090/user\", this.form).then(res => {\n        if (res) {\n          this.$message.success(\"Save Successfully!\");\n          this.dialogFormVisible = false;\n          this.load();\n        } else {\n          this.$message.error(\"Save Unsuccessfully!\");\n        }\n      });\n    },\n    handleAdd() {\n      this.dialogFormVisible = true;\n      this.form = {};\n    },\n    handleEdit(row) {\n      this.form = row;\n      this.dialogFormVisible = true;\n    },\n    del(id) {\n      this.request.delete(\"http://localhost:9090/user/\" + id).then(res => {\n        if (res) {\n          this.$message.success(\"Delete Successfully!\");\n          this.load();\n        } else {\n          this.$message.error(\"Delete Unsuccessfully!\");\n        }\n      });\n    },\n    handleSelectionChange(val) {\n      console.log(val);\n      this.multipleSelection = val;\n    },\n    delBatch() {\n      let ids = this.multipleSelection.map(v => v.id);\n      this.request.post(\"http://localhost:9090/user/del/batch\", ids).then(res => {\n        if (res) {\n          this.$message.success(\"Bulk Delete Successfully!\");\n          this.load();\n        } else {\n          this.$message.error(\"Bulk Delete Unsuccessfully!\");\n        }\n      });\n    },\n    reset() {\n      this.username = \"\";\n      this.email = \"\";\n      this.address = \"\";\n      this.load();\n    },\n    handleSizeChange(pageSize) {\n      console.log(pageSize);\n      this.pageSize = pageSize;\n      this.load();\n    },\n    handleCurrentChange(pageNum) {\n      console.log(pageNum);\n      this.pageNum = pageNum;\n      this.load();\n    },\n    exp() {\n      window.open(\"http://localhost:9090/user/export\");\n    },\n    handleExcelImportSuccess() {\n      this.$message.success(\"Import Successfully!\");\n      this.load();\n    }\n  }\n};","map":{"version":3,"names":["name","data","tableData","total","pageNum","pageSize","username","email","address","form","dialogFormVisible","multipleSelection","created","load","methods","request","get","then","res","console","log","records","save","post","$message","success","error","handleAdd","handleEdit","row","del","id","delete","handleSelectionChange","val","delBatch","ids","map","v","reset","handleSizeChange","handleCurrentChange","exp","window","open","handleExcelImportSuccess"],"sources":["src/views/User.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <div style=\"margin: 10px 0\">\r\n      <el-input style=\"width: 200px\" placeholder=\"Please enter a name\" suffix-icon=\"el-icon-search\" v-model=\"username\"></el-input>\r\n      <el-input style=\"width: 200px\" placeholder=\"Please enter an email\" suffix-icon=\"el-icon-message\" class=\"ml-5\" v-model=\"email\"></el-input>\r\n      <el-input style=\"width: 200px\" placeholder=\"Please enter an address\" suffix-icon=\"el-icon-position\" class=\"ml-5\" v-model=\"address\"></el-input>\r\n      <el-button class=\"ml-5\" type=\"primary\" @click=\"load\">Rearch</el-button>\r\n      <el-button type=\"warning\" @click=\"reset\">Reset</el-button>\r\n    </div>\r\n\r\n    <div style=\"margin: 10px 0\">\r\n      <el-button type=\"primary\" @click=\"handleAdd\">Add1<i class=\"el-icon-circle-plus-outline\"></i></el-button>\r\n      <el-popconfirm\r\n          class=\"ml-5\"\r\n          confirm-button-text='Confirm'\r\n          cancel-button-text='Let me think about it'\r\n          icon=\"el-icon-info\"\r\n          icon-color=\"red\"\r\n          title=\"Are you sure you want to delete these data in bulk?\"\r\n          @confirm=\"delBatch\"\r\n      >\r\n        <el-button type=\"danger\" slot=\"reference\">Bulk Delete<i class=\"el-icon-remove-outline\"></i></el-button>\r\n      </el-popconfirm>\r\n      <el-upload action=\"http://localhost:9090/user/import\" :show-file-list=\"false\" accept=\"xlsx\" :on-success=\"handleExcelImportSuccess\" style=\"display: inline-block\">\r\n  \t    <el-button type=\"primary\" class=\"ml-5\">Import<i class=\"el-icon-bottom\"></i></el-button>\r\n      </el-upload>\r\n\r\n      <el-button type=\"primary\" @click=\"exp\" class=\"ml-5\">Export<i class=\"el-icon-top\"></i></el-button>\r\n    </div>\r\n\r\n    <el-table :data=\"tableData\" border stripe :header-cell-class-name=\"'headerBg'\"  @selection-change=\"handleSelectionChange\">\r\n      <el-table-column type=\"selection\" width=\"55\"></el-table-column>\r\n      <el-table-column prop=\"id\" label=\"ID\" width=\"80\"></el-table-column>\r\n      <el-table-column prop=\"username\" label=\"User Name\" width=\"140\"></el-table-column>\r\n      <el-table-column prop=\"nickname\" label=\"Nick Name\" width=\"120\"></el-table-column>\r\n      <el-table-column prop=\"email\" label=\"Email\"></el-table-column>\r\n      <el-table-column prop=\"phone\" label=\"Phone\"></el-table-column>\r\n      <el-table-column prop=\"address\" label=\"Address\"></el-table-column>\r\n      <el-table-column label=\"Operation\"  width=\"200\" align=\"center\">\r\n        <template slot-scope=\"scope\">\r\n          <el-button type=\"success\" @click=\"handleEdit(scope.row)\">Edit <i class=\"el-icon-edit\"></i></el-button>\r\n          <el-popconfirm\r\n              class=\"ml-5\"\r\n              confirm-button-text='Confirm'\r\n              cancel-button-text='Let me think about it'\r\n              icon=\"el-icon-info\"\r\n              icon-color=\"red\"\r\n              title=\"Are you sure you want to delete this data?\"\r\n              @confirm=\"del(scope.row.id)\"\r\n          >\r\n            <el-button type=\"danger\" slot=\"reference\">Delete <i class=\"el-icon-remove-outline\"></i></el-button>\r\n          </el-popconfirm>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n    <div style=\"padding: 10px 0\">\r\n      <el-pagination\r\n          @size-change=\"handleSizeChange\"\r\n          @current-change=\"handleCurrentChange\"\r\n          :current-page=\"pageNum\"\r\n          :page-sizes=\"[2, 5, 10, 20]\"\r\n          :page-size=\"pageSize\"\r\n          layout=\"total, sizes, prev, pager, next, jumper\"\r\n          :total=\"total\">\r\n      </el-pagination>\r\n    </div>\r\n\r\n    <el-dialog title=\"User Information\" :visible.sync=\"dialogFormVisible\" width=\"30%\" >\r\n      <el-form label-width=\"80px\" size=\"small\">\r\n        <el-form-item label=\"User Name\">\r\n          <el-input v-model=\"form.username\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"Nick Name\">\r\n          <el-input v-model=\"form.nickname\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"Email\">\r\n          <el-input v-model=\"form.email\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"Phone\">\r\n          <el-input v-model=\"form.phone\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"Address\">\r\n          <el-input v-model=\"form.address\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n      </el-form>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"dialogFormVisible = false\">Cancel</el-button>\r\n        <el-button type=\"primary\" @click=\"save\">Confirm</el-button>\r\n      </div>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"User\",\r\n  data() {\r\n    return {\r\n      tableData: [],\r\n      total: 0,\r\n      pageNum: 1,\r\n      pageSize: 10,\r\n      username: \"\",\r\n      email: \"\",\r\n      address: \"\",\r\n      form: {},\r\n      dialogFormVisible: false,\r\n      multipleSelection: []\r\n    }\r\n  },\r\n  created() {\r\n    this.load()\r\n  },\r\n  methods : {\r\n    load() {\r\n      this.request.get(\"http://localhost:9090/user/page?pageNum=\"+this.pageNum+\r\n          \"&pageSize=\"+this.pageSize+\"&username=\"+this.username+\r\n          \"&email=\"+this.email +\"&address=\"+this.address).then(res=> {\r\n        console.log(res)\r\n        this.tableData = res.records\r\n        this.total = res.total\r\n      })\r\n    },\r\n    save() {\r\n      this.request.post(\"http://localhost:9090/user\", this.form).then(res => {\r\n        if (res) {\r\n          this.$message.success(\"Save Successfully!\")\r\n          this.dialogFormVisible = false\r\n          this.load()\r\n        } else {\r\n          this.$message.error(\"Save Unsuccessfully!\")\r\n        }\r\n      })\r\n    },\r\n    handleAdd() {\r\n      this.dialogFormVisible = true\r\n      this.form = {}\r\n    },\r\n    handleEdit(row) {\r\n      this.form = row\r\n      this.dialogFormVisible = true\r\n    },\r\n    del(id) {\r\n      this.request.delete(\"http://localhost:9090/user/\" + id).then(res => {\r\n        if (res) {\r\n          this.$message.success(\"Delete Successfully!\")\r\n          this.load()\r\n        } else {\r\n          this.$message.error(\"Delete Unsuccessfully!\")\r\n        }\r\n      })\r\n    },\r\n    handleSelectionChange(val) {\r\n      console.log(val)\r\n      this.multipleSelection = val\r\n    },\r\n    delBatch() {\r\n      let ids = this.multipleSelection.map(v => v.id)\r\n      this.request.post(\"http://localhost:9090/user/del/batch\", ids).then(res => {\r\n        if (res) {\r\n          this.$message.success(\"Bulk Delete Successfully!\")\r\n          this.load()\r\n        } else {\r\n          this.$message.error(\"Bulk Delete Unsuccessfully!\")\r\n        }\r\n      })\r\n    },\r\n    reset() {\r\n      this.username = \"\"\r\n      this.email = \"\"\r\n      this.address = \"\"\r\n      this.load()\r\n    },\r\n    handleSizeChange(pageSize) {\r\n      console.log(pageSize)\r\n      this.pageSize = pageSize\r\n      this.load()\r\n    },\r\n    handleCurrentChange(pageNum) {\r\n      console.log(pageNum)\r\n      this.pageNum = pageNum\r\n      this.load()\r\n    },\r\n    exp() {\r\n      window.open(\"http://localhost:9090/user/export\")\r\n    },\r\n    handleExcelImportSuccess() {\r\n      this.$message.success(\"Import Successfully!\")\r\n      this.load()\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style>\r\n.headerBg {\r\n  background: #eee!important;\r\n}\r\n</style>\r\n"],"mappings":"AA8FA;EACAA,IAAA;EACAC,KAAA;IACA;MACAC,SAAA;MACAC,KAAA;MACAC,OAAA;MACAC,QAAA;MACAC,QAAA;MACAC,KAAA;MACAC,OAAA;MACAC,IAAA;MACAC,iBAAA;MACAC,iBAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,IAAA;EACA;EACAC,OAAA;IACAD,KAAA;MACA,KAAAE,OAAA,CAAAC,GAAA,mDAAAZ,OAAA,GACA,oBAAAC,QAAA,uBAAAC,QAAA,GACA,iBAAAC,KAAA,sBAAAC,OAAA,EAAAS,IAAA,CAAAC,GAAA;QACAC,OAAA,CAAAC,GAAA,CAAAF,GAAA;QACA,KAAAhB,SAAA,GAAAgB,GAAA,CAAAG,OAAA;QACA,KAAAlB,KAAA,GAAAe,GAAA,CAAAf,KAAA;MACA;IACA;IACAmB,KAAA;MACA,KAAAP,OAAA,CAAAQ,IAAA,oCAAAd,IAAA,EAAAQ,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA;UACA,KAAAM,QAAA,CAAAC,OAAA;UACA,KAAAf,iBAAA;UACA,KAAAG,IAAA;QACA;UACA,KAAAW,QAAA,CAAAE,KAAA;QACA;MACA;IACA;IACAC,UAAA;MACA,KAAAjB,iBAAA;MACA,KAAAD,IAAA;IACA;IACAmB,WAAAC,GAAA;MACA,KAAApB,IAAA,GAAAoB,GAAA;MACA,KAAAnB,iBAAA;IACA;IACAoB,IAAAC,EAAA;MACA,KAAAhB,OAAA,CAAAiB,MAAA,iCAAAD,EAAA,EAAAd,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA;UACA,KAAAM,QAAA,CAAAC,OAAA;UACA,KAAAZ,IAAA;QACA;UACA,KAAAW,QAAA,CAAAE,KAAA;QACA;MACA;IACA;IACA